<%- include("partials/header"); %>
<h1>Home</h1>
<p><%=startingContent%></p>

<%if(posts.length > 0){ posts.forEach((post) => { %>

<h2><%=post.title%></h2>
<p>
  <%if(post.content){ %> <%=post.content.substring(0,100)+"..."%><a
    href="/posts/<%=post._id%>"
    class="text-cadetBlue"
  >
    Read More</a
  >
  <% } %>
</p>

<%});}else{ %>
<p>No posts found.</p>
<% } %> <%- include("partials/footer"); %>
